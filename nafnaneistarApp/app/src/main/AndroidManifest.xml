<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.nafnaneistar.loginactivity">

    <application
        android:name="xyz.nafnaneistar.controller.ApiController"
        android:allowBackup="true"
        android:icon="@drawable/fill"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginActivity"
        android:usesCleartextTraffic="true">
        <activity android:name="xyz.nafnaneistar.activities.SwipeActivity"></activity>
        <activity android:name="xyz.nafnaneistar.activities.SignupActivity" />
        <activity android:name="xyz.nafnaneistar.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>